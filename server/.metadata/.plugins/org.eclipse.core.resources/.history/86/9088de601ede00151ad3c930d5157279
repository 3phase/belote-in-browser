package org.phase.game.entities;

public class Game {

	private long gameId;
	private long announce;
	private int team_one_result;
	private int team_two_result;
	private Table table = new Table();
	
	public Game(int t1res, int t2res) {
		team_one_result = t1res;
		team_two_result = t2res;
	}
	
	public long get_announde() {
		return announce;
	}
	
	public void set_announce(long announce_, Team player) {
		announce = announce_;
	}
	
}
